{
    "name": "thebigcrafter/hyperbackup",
    "description": "HyperBackup lets you backup your PocketMine-MP server data such as plugins, worlds, and server configurations.",
    "type": "project",
    "require": {
        "php": "^8.0",
        "cortexpe/commando": "dev-master",
        "nelexa/zip": "^4.0"
    },
    "require-dev": {
        "pocketmine/pocketmine-mp": "^4.3",
        "phpstan/phpstan": "^1.5",
        "friendsofphp/php-cs-fixer": "^3.8"
    },
    "license": "GPL-3.0-only",
    "autoload": {
        "psr-4": {
            "thebigcrafter\\HyperBackup\\": "src/"
        }
    },
	"scripts": {
		"analyse": "./vendor/bin/phpstan analyse -c phpstan.neon.dist"
	},
    "authors": [
        {
            "name": "MintoD",
            "email": "tobythedev7@outlook.com"
        }
    ]
}
